<% if params[:last] %>
	$('#dates > tbody').append("<%= escape_javascript(render :partial => 'date') %>");
	$('#dates').attr('last', '<%= @last_date%>');
<% elsif params[:first] %>
	$('#dates > tbody').prepend("<%= escape_javascript(render :partial => 'date') %>");
	$('#dates').attr('first', '<%= @first_date%>');
	window.scrollTo(0,150);
<% end %>
